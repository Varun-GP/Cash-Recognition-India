python3 /home/gason/tensorflow/tensorflow/examples/image_retraining/retrain.py   --bottleneck_dir=/home/gason/demo_ltts/cash_recognition/junk   --how_many_training_steps 3000   --model_dir=/home/gason/demo_ltts/cash_recognition/models   --output_graph=/home/gason/demo_ltts/cash_recognition/models/cash_recog.pb   --output_labels=/home/gason/demo_ltts/cash_recognition/models/labels.txt  --image_dir /home/gason/demo_ltts/cash_recognition/data/



python3 /home/gason/tensorflow/tensorflow/python/tools/optimize_for_inference.py --input=/home/gason/demo_ltts/cash_recognition/models/cash_recog.pb --output=/home/gason/demo_ltts/cash_recognition/models/optimized.pb --input_names="Mul" --output_names="final_result"

######################################
python3 /home/gason/tensorflow-for-poets-2/scripts/quantize_graph.py --input=/home/gason/demo_ltts/cash_recognition/models/optimized.pb --output=home/gason/demo_ltts/cash_recognition/models/round_optimized.pb --output_node_names=final_result --mode=weights_rounded
