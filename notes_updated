sudo cp /home/gason/demo_ltts/cash_recognition/models/optimized.pb /home/gason/demo_ltts/cash_recognition/models/labels.txt /home/gason/demo_ltts/cash_recognition/tensorflow-classifier-android/app/src/main/assets/


